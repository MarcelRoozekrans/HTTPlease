{
    "version": "1.0.0-*",

    "buildOptions": {
        "xmlDoc": true,
        "publicSign": true,
		"keyFile": "../../HTTPlease.snk"
    },
    
    "packOptions": {
        "title": "HTTPlease.Security",
        "description": "Security features for HTTPlease",
        "summary": "Security features (e.g. authentication) for HTTPlease",

        "authors": [
            "tintoy@tintoy.io"
        ],
        "copyright": "Copyright © Adam Friedman 2016",
        "owners": [
            "tintoy@tintoy.io"
        ],
        "tags": [
            "HTTP",
            "HTTPlease",
            "Security",
            "Authentication"
        ],
        "iconUrl": "https://tintoy.github.io/HTTPlease/_static/logo-nuget.png",
        "projectUrl": "https://tintoy.github.io/HTTPlease/",
        "repository": {
            "type": "git",
            "url": "https://github.com/tintoy/HTTPlease"
        },
        "licenseUrl": "https://raw.githubusercontent.com/tintoy/HTTPlease/development/r1.0/LICENSE"        

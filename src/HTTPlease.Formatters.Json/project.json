{
    "version": "1.0.0-*",
    
    "packOptions": {
        "title": "HTTPlease JSON Formatter",
        "summary": "JSON content formatter (serialisation) support for HTTPlease",
        "description": "JSON content formatter (serialisation) functionality for HTTPlease Immutable HTTP templates",
        "authors": [
            "tintoy@tintoy.io"
        ],
        "copyright": "Copyright © Adam Friedman 2016",
        "owners": [
            "tintoy@tintoy.io"
        ],
        "tags": [
            "HTTP",
            "HTTPlease",
            "Formatters",
            "Serialization",
            "JSON"
        ],
        "projectUrl": "https://tintoy.github.io/HTTPlease/",
        "licenseUrl": "https://raw.githubusercontent.com/tintoy/HTTPlease/development/r1.0/LICENSE"
    },

    "tooling": {
        "defaultNamespace": "HTTPlease.Formatters.Json"
    },

    "frameworks": {
        "netstandard1.3": { 
            "imports": [
				"dotnet5.6",
				"dnxcore50",
				"portable-net45+win8"
			],
			"dependencies": {
				"Microsoft.CSharp": "4.0.1",
				"System.Collections": "4.0.11",
                "System.Collections.Specialized": "4.0.1",
				"System.Linq": "4.1.0",
                "System.Runtime": "4.1.0",
                "System.Text.RegularExpressions": "4.1.0",
				"System.Threading": "4.0.11"
			}
        }
    },

    "dependencies": {
        "HTTPlease.Formatters": "1.0.0-*",
        "Newtonsoft.Json": "8.0.3",
        "System.Collections.Concurrent": "4.0.12",
        "System.Collections.Immutable": "1.2.0",
        "System.Net.Http": "4.1.0"
    }
}

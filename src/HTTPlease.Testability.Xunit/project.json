{
    "version": "1.0.0-*",
    
    "packOptions": {
        "title": "HTTPlease Testability",
        "summary": "Testability support for HTTPlease",
        "description": "Request / response mocking and other test-related functionality for HTTPlease Immutable HTTP templates",
        "authors": [
            "tintoy@tintoy.io"
        ],
        "copyright": "Copyright © Adam Friedman 2016",
        "owners": [
            "tintoy@tintoy.io"
        ],
        "tags": [
            "HTTP",
            "HTTPlease",
            "Test",
            "Testability"
        ],
        "projectUrl": "https://tintoy.github.io/HTTPlease/",
        "licenseUrl": "https://raw.githubusercontent.com/tintoy/HTTPlease/development/r1.0/LICENSE"
    },

    "tooling": {
        "defaultNamespace": "HTTPlease.Testability"
    },

    "frameworks": {
